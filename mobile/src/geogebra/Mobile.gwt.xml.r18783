<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="mobile" >

    <source path="mobile" />

    <!-- Inherit the core Web Toolkit stuff. -->

    <inherits name="com.google.gwt.user.User" />

    <!-- Inherits Web Hopefully it inherits everything that is in it, and even Common -->

    <inherits name="geogebra.Common" />

    <inherits name="geogebra.Web" />

    <!-- Other module inherits -->

    <inherits name="com.google.gwt.http.HTTP" />

    <inherits name="com.googlecode.gwtphonegap.PhoneGap" />

    <inherits name="com.googlecode.mgwt.MGWT" />

    <!-- Specify the app entry point class. -->

    <entry-point class="geogebra.mobile.Mobile" />

    <!-- Compile only for Safari now -->

    <set-property
        name="user.agent"
        value="safari" />

    <set-configuration-property
        name="mgwt.css"
        value="pretty" />

    <set-configuration-property
        name="mobile.devmode.js"
        value="pretty" />

</module>