<FindBugsFilter>
     <Match>
       <Bug pattern="EI_EXPOSE_REP" />
     </Match>
    <Match>
       <Bug pattern="EI_EXPOSE_REP2" />
     </Match>
    <Match>
       <Bug pattern="MS_EXPOSE_REP" />
     </Match>
    <Match>
        <!-- do not report this for all the factory prototype setters -->
        <Class name="~.*(Factory|StringUtil|Log)" />
        <Bug pattern="EI_EXPOSE_STATIC_REP2" />
    </Match>
    <Match>
         <!-- https://github.com/spotbugs/spotbugs/issues/1539 -->
         <Bug pattern="DMI_RANDOM_USED_ONLY_ONCE" />
    </Match>
       <Match>
       <Bug pattern="SE_BAD_FIELD" />
     </Match>
     <Match>
       <Bug pattern="SE_BAD_FIELD_STORE" />
     </Match>
      <Match>
       <Bug pattern="SE_COMPARATOR_SHOULD_BE_SERIALIZABLE" />
     </Match>
     <Match>
      <Package name="~org\.apache.*" />
    </Match>
     <Match>
      <Package name="~edu\.uci.*" />
    </Match>
     <Match>
      <Package name="~com\.github.*" />
    </Match>
     <Match>
      <Package name="~com\.google.*" />
    </Match>
    <Match>
      <Package name="~android\.support.*" />
    </Match>
     <Match>
      <Package name="~(org.geogebra.common.kernel.(parser|prover.polynomial)|com.himamis.retex.editor.share.io.latex)" />
      <Or>
        <Bug pattern="DLS_DEAD_LOCAL_STORE" />    
        <Bug pattern="DM_DEFAULT_ENCODING" />
        <Bug pattern="MS_OOI_PKGPROTECT" />
        <Bug pattern="MS_PKGPROTECT" />
        <Bug pattern="MS_SHOULD_BE_FINAL" />    
        <Bug pattern="NM_METHOD_NAMING_CONVENTION" />    
        <Bug pattern="SF_SWITCH_NO_DEFAULT" />
        <Bug pattern="UCF_USELESS_CONTROL_FLOW" />    
        <Bug pattern="URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD" />
        <Bug pattern="UC_USELESS_VOID_METHOD"/>
        <Bug pattern="URF_UNREAD_FIELD"/>
      </Or>
    </Match>
    <Match>
      <Or>
        <Class name="~.*\.R\$.*"/>
        <Class name="~.*\.Manifest\$.*"/>
        </Or>
    </Match>
    <Match>
      <Method name="getInstance_" />
    </Match>
</FindBugsFilter>
