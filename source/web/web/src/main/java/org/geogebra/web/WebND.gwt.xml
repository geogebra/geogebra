<?xml version="1.0" encoding="UTF-8"?>
<module>

    <inherits name='org.geogebra.web.Html5'/>
    <inherits name='org.geogebra.CommonKeyboard'/>
    <inherits name='org.geogebra.keyboard.KeyboardWeb'/>
    <inherits name='fr.grenoble.ujf.Giac'/>
    <inherits name='org.geogebra.web.Carota'/>
    <inherits name='org.geogebra.Multiplayer'/>

    <!-- Specify the paths for translatable code                    -->
    <source path="full"/>
    <source path="editor"/>
