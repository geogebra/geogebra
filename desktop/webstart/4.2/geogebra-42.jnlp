<?xml version="1.0" encoding="utf-8"?>
<!-- JNLP File for GeoGebra WebStart Application -->
<jnlp spec="1.0+" codebase="http://www.geogebra.org/webstart/4.2/" href="geogebra-42.jnlp">
  <information>
    <title>GeoGebra 4.2 Beta</title>
    <vendor>International GeoGebra Institute</vendor>
    <homepage href="http://www.geogebra.org/"/>
    <description>Dynamic Mathematics for Everyone</description>
    <icon href="http://www.geogebra.org/static/images/ggb-logo-64x64-white.png" width="64" height="64"/>
    <offline-allowed/>
    <shortcut online="true">
      <desktop/>
      <menu submenu="GeoGebra 4.2 Beta"/>
    </shortcut>

    <association mime-type="application/vnd.geogebra.file" extensions="ggb" description="GeoGebra File"/>
    <association mime-type="application/vnd.geogebra.tool" extensions="ggt" description="GeoGebra Tool"/>

  <related-content href="http://www.geogebra.org/">
      <title>www.geogebra.org</title>
      <description>www.geogebra.org</description>
    </related-content>
    <related-content href="http://www.geogebra.org/forum/">
      <title>GeoGebra Forum</title>
      <description>GeoGebra Forum</description>
      <icon href="../forum.gif" width="16" height="16"/>
    </related-content>
    <related-content href="http://wiki.geogebra.org//">
      <title>GeoGebra Wiki</title>
      <description>GeoGebra Wiki</description>
      <icon href="../wiki.jpg" width="16" height="16"/>
    </related-content>
    <related-content href="http://www.geogebratube.org/">
      <title>GeoGebraTube</title>
      <description>GeoGebraTube</description>
      <icon href="../wiki.jpg" width="16" height="16"/>
    </related-content>
  </information>
  <information locale="de">
    <description>Dynamische Mathematik f√ºr Alle</description>
    <offline-allowed/>
  </information>
  <security>
    <all-permissions/>
  </security>
	<update check="timeout" policy="always"/>
  <resources>
    <property name="jnlp.packEnabled" value="true"/>
    <j2se version="1.5.0+" max-heap-size="512m" href="http://java.sun.com/products/autodl/j2se"/>
    <jar href="http://www.geogebra.org/webstart/4.2/jnlp/geogebra.jar" main="true"/>
    <jar href="http://www.geogebra.org/webstart/4.2/jnlp/geogebra_main.jar"/>
    <jar href="http://www.geogebra.org/webstart/4.2/jnlp/geogebra_gui.jar"/>
    <jar href="http://www.geogebra.org/webstart/4.2/jnlp/geogebra_properties.jar"/>
    <jar href="http://www.geogebra.org/webstart/4.2/jnlp/geogebra_export.jar"/>    
    <jar href="http://www.geogebra.org/webstart/4.2/jnlp/geogebra_algos.jar"/>    
    <jar href="http://www.geogebra.org/webstart/4.2/jnlp/geogebra_cas.jar"/>
    <jar href="http://www.geogebra.org/webstart/4.2/jnlp/geogebra_usb.jar"/>
    <jar href="http://www.geogebra.org/webstart/4.2/jnlp/geogebra_javascript.jar"/>   
    <jar href="http://www.geogebra.org/webstart/4.2/jnlp/jlatexmath.jar"/>
    <jar href="http://www.geogebra.org/webstart/4.2/jnlp/jlm_greek.jar"/>
    <jar href="http://www.geogebra.org/webstart/4.2/jnlp/jlm_cyrillic.jar"/>
    <jar href="http://www.geogebra.org/webstart/4.2/jnlp/jython.jar"/>
  </resources>
  
    <!-- USB native components (architecture dependent) -->
    <resources os="Windows" arch="x86">
      <nativelib href = "http://www.geogebra.org/webstart/4.2/jnlp/openni.jar" />
      <nativelib href = "http://www.geogebra.org/webstart/4.2/jnlp/vernier_ccsd-win.jar" />
      <nativelib href = "http://www.geogebra.org/webstart/4.2/jnlp/jd2xx.jar" />
    </resources>
    <resources os="Windows" arch="amd64">
      <nativelib href = "http://www.geogebra.org/webstart/4.2/jnlp/openni64.jar" />
      <nativelib href = "http://www.geogebra.org/webstart/4.2/jnlp/vernier_ccsd-win.jar" />
      <nativelib href = "http://www.geogebra.org/webstart/4.2/jnlp/jd2xx64.jar" />
    </resources>
    <resources os="Windows" arch="x86_64">
      <nativelib href = "http://www.geogebra.org/webstart/4.2/jnlp/openni64.jar" />
      <nativelib href = "http://www.geogebra.org/webstart/4.2/jnlp/vernier_ccsd-win.jar" />
      <nativelib href = "http://www.geogebra.org/webstart/4.2/jnlp/jd2xx64.jar" />
    </resources>
    <resources os="Mac OS X" arch="ppc">
      <nativelib href = "http://www.geogebra.org/webstart/4.2/jnlp/vernier_ccsd-mac-static.jar" />
    </resources>
    <resources os="Mac OS X" arch="i386">
      <nativelib href = "http://www.geogebra.org/webstart/4.2/jnlp/vernier_ccsd-macintel-static.jar" />
    </resources>
    <resources os="Mac OS X" arch="x86_64">
      <nativelib href = "http://www.geogebra.org/webstart/4.2/jnlp/vernier_ccsd-macintel-static.jar" />
    </resources>
    
  <application-desc main-class="geogebra.GeoGebra">
  </application-desc>
</jnlp>
