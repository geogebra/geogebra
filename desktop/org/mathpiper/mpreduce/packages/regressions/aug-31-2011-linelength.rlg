lisp;


nil


linelength 60;


80


symbolic procedure test_wrap x;
  begin
    scalar a, b;
    for i := 1:60 do <<
      terpri();
      prin2 "Test at offset "; print i;
      for j := 1:i-1 do prin2 ".";
      prin2 "<";
      a := posn();
      prin1 x;
      b := posn();
      prin2 ">";
      terpri();
      if b > 60 then print list('overflowed, a, b)
      else print list("positions", a, b) >>;
    print "done"
  end;


