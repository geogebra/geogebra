Macro("xPrimitivePart",{poly,xcont})
[
    Local(pp);
    If(InVerboseMode(),Tell("  xPrimitivePart2",{poly,xcont}));
    
    If( IsBound(@xcont), 
      [
          pp := Eval(@poly) / Eval(@xcont);
      ],
      [
          Local(xCont);
          xCont  := xContent(Eval(@poly));
          @xcont := xCont;
          pp     := Eval(@poly) / xCont;
      ]
    );
    pp;
    
];

