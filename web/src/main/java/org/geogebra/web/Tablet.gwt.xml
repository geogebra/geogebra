<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='tablet'>
  <inherits name='org.geogebra.web.Touch'/>

  <!-- Specify the app entry point class.                         -->
  <entry-point class='org.geogebra.web.tablet.Tablet'/>

  <source path='tablet'/>
  
  <replace-with class="org.geogebra.web.full.gui.applet.AppletFactory2D">
    <when-type-is class="org.geogebra.web.full.gui.applet.AppletFactory" />    
  </replace-with>
  
  <!-- Compile only one permutation -->
  <collapse-all-properties />
</module>
