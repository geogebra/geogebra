<launch4jConfig>
  <headerType>gui</headerType>
  <dontWrapJar>true</dontWrapJar>
  <errTitle>GeoGebraSMART</errTitle>
  <icon>geogebra.ico</icon>
  <classPath>
   <cp>%EXEDIR%\geogebra-jogl1.jar</cp>
   <mainClass>geogebra.iwb.GeoGebraIWB</mainClass>
  </classPath>
  <jre>
    <minVersion>1.5.0</minVersion>
    <initialHeapSize>32</initialHeapSize>
    <maxHeapSize>512</maxHeapSize>
    <opt>-Djava.library.path="%EXEDIR%"</opt>
  </jre>
  <cmdLine></cmdLine>
  <versionInfo>
    <fileDescription>GeoGebra &amp; SMART Board</fileDescription>
    <copyright>(C) 2001-2012 International GeoGebra Institute</copyright>
    <productName>GeoGebra &amp; SMART Board</productName>
    <companyName>International GeoGebra Institute</companyName>
    <internalName>geogebrasmart</internalName>
    <originalFilename>GeoGebraSMART.exe</originalFilename>
  </versionInfo>
  <messages>
    <jreVersionErr>This application requires a Java Runtime Environment newer or equal to</jreVersionErr>
  </messages>
</launch4jConfig>
